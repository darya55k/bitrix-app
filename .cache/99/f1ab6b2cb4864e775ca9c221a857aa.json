__label {\r\n    font-weight: 600;\r\n    color: rgba(64, 70, 84, 1);\r\n}\r\n\r\n.form__title {\r\n    margin: 0 auto;\r\n    max-width: 464px;\r\n    padding-bottom: 70px;\r\n}\r\n\r\n.form__content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    margin: 0 auto;\r\n    width: 400px;\r\n}\r\n\r\n.form__button {\r\n    width: 128px;\r\n    height: 52px;\r\n    background: #7CABFF;\r\n    border-radius: 15px;\r\n    color: white;\r\n    border: none;\r\n    cursor: pointer;\r\n}\r\n\r\n.card__item {\r\n    padding: 20px 40px;\r\n    border-radius: 23px;\r\n    margin-bottom: 40px;\r\n}\r\n\r\n.card__item-red {\r\n\r\n    box-shadow: 0px 4px 4px 0px rgba(241, 72, 104, 0.3);\r\n    border: 2px solid rgba(241, 70, 104, 1);\r\n\r\n}\r\n\r\n.card__item-red::before {\r\n    background: linear-gradient(to bottom, rgba(241, 70, 104, 1) 0%, rgba(241, 160, 70, 1) 100%);\r\n\r\n}\r\n\r\n.card__item-blue {\r\n    border: 2px solid rgba(102, 154, 255, 1);\r\n    /* border-image: linear-gradient(to bottom, rgba(102, 154, 255, 1) 0%, rgba(60, 254, 255, 1) 100%); */\r\n    box-shadow: 0px 4px 4px 0px rgba(101, 155, 255, 0.3);\r\n\r\n\r\n}\r\n\r\n.card__item-green {\r\n\r\n    box-shadow: 0px 4px 4px 0px rgba(63, 215, 146, 1);\r\n    border: 2px solid rgba(63, 215, 146, 1);\r\n    /* border-image: linear-gradient(to bottom, rgba(63, 215, 146, 1) 0%, rgba(187, 241, 101, 1) 100%); */\r\n\r\n    box-shadow: 0px 4px 4px 0px rgba(64, 215, 146, 0.3);\r\n\r\n}\r\n\r\n.card__content {\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n\r\n.card__status {\r\n    width: 123px;\r\n    height: 20px;\r\n    font-size: 14px;\r\n    border-radius: 23px;\r\n    color: white;\r\n    margin: 0;\r\n    padding: 11px 20px;\r\n    text-align: center;\r\n    cursor: pointer;\r\n}\r\n\r\n.card__status-red {\r\n    background: linear-gradient(to bottom, rgba(241, 70, 104, 1) 0%, rgba(241, 160, 70, 1) 100%);\r\n}\r\n\r\n.card__status-green {\r\n    background: linear-gradient(to bottom, rgba(63, 215, 146, 1) 0%, rgba(187, 241, 101, 1) 100%);\r\n}\r\n\r\n.card__status-blue {\r\n    background: linear-gradient(to bottom, rgba(102, 154, 255, 1) 0%, rgba(60, 254, 255, 1) 100%);\r\n}\r\n\r\n.form__label-index {\r\n    padding-bottom: 10px;\r\n}\r\n\r\n.form__input-textarea {\r\n    min-height: 70px;\r\n}"},"lineCount":169}},"error":null,"hash":"66c131a0a9df0bb09a4c9f5558d876e7","cacheData":{"env":{}}}